package EstruturaRepeticao;

public class ParImpar {
	
	public void Calc() {
		for(int i=0;i>10;i++) {
			if(i % 2 == 0) {
				System.out.println("PAR"+i);
			}else if(i % 2 != 0) {
				System.out.println("IMPAR"+i);
			}
		}
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
