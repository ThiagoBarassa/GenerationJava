package ControlFlow2;

import java.util.Scanner;

public class Maior {
	public int n1,n2,maior;
	private Scanner ler;
	
	public void Print() {
		ler = new Scanner(System.in);
		System.out.println("Digite um numero: ");
		n1 = ler.nextInt();
		System.out.println("Digite outro numero: ");
		n2 = ler.nextInt();
	}
	public void Calc() {
		if(n1!=n2) {
			
			maior = n1 < n2 ? (maior = n1) : (n1 > n2 ?  maior == n2);
			 status = salario < 1000 ? salario += (salario *= 0.5) : (salario >= 1000 && salario < 2000);
			System.out.println(maior);
		}
	}
	public static void main(String[] args) {
