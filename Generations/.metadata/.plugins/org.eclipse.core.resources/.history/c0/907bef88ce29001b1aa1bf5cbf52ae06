package EstruturaRepeticao;

public class Contador {
	int c=233;
	public void Calc() {
		do {
			if(c<=300 && c>=400) {
				c+=3;
				System.out.println(c+=3);
			}
			System.out.println(c+=5);
		}while(c>=456);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Contador c = new Contador();
		c.Calc();
	}

}
